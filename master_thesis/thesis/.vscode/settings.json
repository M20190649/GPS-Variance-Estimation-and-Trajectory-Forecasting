{
  "latex-workshop.latex.toolchain": [
    {
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    }, {
      "command": "biber",
      "args": [
        "%DOCFILE%"
      ]
    }, {
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "%DOC%"
      ]
    }
  ],
  "latex-workshop.latex.additionalBib": [
    "references.bib"
  ],
  "spellright.language": "en_GB",
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "plaintext"
  ],
  "latex-workshop.debug.showUpdateMessage": false
}